onst    [personne, setPersonne] =useState([]);\r\n     \r\n\r\n    \r\n    \r\n     const errorToast =()=>{\r\n        toast(\"wrong username or password \",{\r\n            className:\"custom-toast\",\r\n            draggable: true,\r\n            position : toast.POSITION.BUTTON_CENTER\r\n        });\r\n        };\r\n\r\n     const changeUrl =()=>{\r\n         History.push(\"/home\"); \r\n     }\r\n     \r\n\r\n     const Join =()=>{\r\n     Axios.post('http://192.168.4.102:4000/login',{\r\n    username: username,\r\n    password:password,\r\n \r\n     }).then (result=>{\r\n     \r\n      if (result.data == \"Login ou mot de passe incorrecte .\" )\r\n      {\r\n        setMessage(result.data);\r\n        setShow(true); \r\n      }\r\n      else {\r\n          //console.log(result.data.result[0]);\r\n          localStorage.setItem('token',result.data.token);\r\n          user=result.data.result[0];\r\n          var salah ={id: user.id, nom: user.nom, prenom:user.prenom };\r\n          localStorage.setItem('user',JSON.stringify(salah));\r\n          setPersonne(result.data);\r\n          \r\n          \r\n           changeUrl();  \r\n             }\r\n    }\r\n    )};\r\n     const handleSubmit =(e)=>{\r\n        e.preventDefault();\r\n    \r\n        setPassword(e.target.reset());\r\n }\r\n    return ( \r\n\r\n        <Container fluid=\"xl\">\r\n        \r\n        \r\n        <Row className=\"mak\">\r\n        <Col className=\"mak\">\r\n                <h1 className=\"display-4\">Se connecter<br></br><small className=\"text-muted\">Bienvenue</small></h1>\r\n                <Form onSubmit={handleSubmit}>\r\n                <Form.Group controlId=\"formBasicEmail\">\r\n                    <Form.Label>Identifiant</Form.Label>\r\n                    <Form.Control type=\"Identifiant\" placeholder=\"Identifiant\"  onChange={(e)=>{setUsername(e.target.value)}} required />                    \r\n                </Form.Group>\r\n                <Form.Group controlId=\"formBasicPassword\">\r\n                    <Form.Label>Mot de passe </Form.Label>\r\n                \r\n                    <Form.Control type=\"password\" placeholder=\"Mot de passe\"  onChange={(e)=>{setPassword(e.target.value)}}  required/>\r\n                </Form.Group>\r\n                <Alert show={show} variant=\"danger\">\r\n                    <Alert.Heading>{message}</Alert.Heading>\r\n                </Alert>\r\n                <Form.Group className=\"text-center\">\r\n                <Button variant=\"primary\" type=\"submit\" onClick={Join} >\r\n                    Se connecter\r\n                </Button>\r\n                </Form.Group>\r\n                </Form>\r\n          \r\n        </Col>\r\n        <Col>\r\n            <img src={img1} width=\"100%\"></img>\r\n        </Col>\r\n        </Row>\r\n            \r\n\r\n            </Container>\r\n    )\r\n}\r\n\r\nexport default Signin;","/var/www/html/ProjetChat/client/src/component/auth/Signup.js",["60","61","62","63","64","65"],"import React,{useState} from 'react'\r\nimport Axios from 'axios';\r\nimport { Form,Button,Alert,Container,Row,Col,Card } from 'react-bootstrap';\r\n\r\n\r\n   const Signup =()=> {\r\n   const [nom,setNom]= useState(\"\");\r\n   const [prenom,setPrenom]= useState(\"\");\r\n   const [login,setLogin]= useState(\"\");\r\n   const [mdp,setMdp]= useState(\"\");\r\n   const Ajout =()=>{\r\n    Axios.post('http://192.168.4.102:4000/create',{\r\nnom: nom,\r\nprenom:prenom,\r\nlogin:login,\r\nmdp:mdp,\r\n    }).then ((response)=>{\r\n        console.log (response.message);\r\n    })};\r\n\r\n\r\n    return (\r\n        <div>\r\n            <h2>Ajouter un Utilisateur</h2>\r\n\r\n            <Form.Group controlId=\"formBasicEmail\">\r\n                    <Form.Label>Nom</Form.Label>\r\n                    <Form.Control type=\"text\" placeholder=\"Tapez le Nom\"  onChange={(e)=>{setNom(e.target.value)}} required />                    \r\n            </Form.Group>\r\n\r\n\r\n\r\n    